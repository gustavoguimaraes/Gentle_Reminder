<div class = "col-md-6">
	<h1> Welcome Back! </h1>
	<%= form_for(:session, url: sessions_path) do |f| %>
			<%= f.label :email %>
			<%= f.text_field :email %>
			<%= f.label :password %>
			<%= f.password_field :password %>
			<%= f.submit "Sign in"%>
	<% end %>
		<p>New user? <%= link_to "Click Here!", signup_path %></p>
		
</div>


